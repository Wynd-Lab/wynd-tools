{
  "rulesDirectory": ["tslint-plugin-prettier"],
  "extends": [
    "tslint:latest",
    "tslint-config-airbnb",
    "tslint-react",
    "./wynd.json",
    "tslint-config-prettier"
  ],
  "rules": {
    "jsx-boolean-value": [true, "never"],
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false
  }
}