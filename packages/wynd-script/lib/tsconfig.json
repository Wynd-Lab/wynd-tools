{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "../dist",
    "pretty": true,
    "resolveJsonModule": true,
    "strict": true,
    "target": "es2017",
    "lib": [
      "esnext"
    ]
  }
}